@page "/"
@inject IJSRuntime JSRuntime

<h1>Hello, world!</h1>

Welcome to your new app.

<SurveyPrompt Title="How is Blazor working for you?"/>


<button @onclick="DoJsInterop">Do the JS interop thing</button>
<p>@PContents</p>

@code {
    private string PContents { get; set; }
    private async Task DoJsInterop()
    {
        string testing = await JSRuntime.InvokeAsync<string>("JSInteropTest");
        PContents = testing;
    }
}